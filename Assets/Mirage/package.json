{
    "name": "com.mindblocks3.mirage",
    "displayName": "Mirage",
    "version": "100.1.0",
    "unity": "2022.1",
    "description": "Mirage is a high level Networking API for Unity, supporting different low level Transports.",
    "author": "Mirage",
    "repository": {
        "type": "git",
        "url": "https://github.com/Mindblocks3/Mirage.git"
    },
    "dependencies": {
        "com.unity.nuget.mono-cecil": "1.10.2",
        "com.cysharp.unitask": "2.3.1"
    },
    "samples": [
        {
            "displayName": "AdditiveScenes",
            "description": "Adding and removing scenes in multiplayer games",
            "path": "Samples~/AdditiveScenes"
        },
        {
            "displayName": "Basic",
            "description": "Demonstrate simple setup and synchronizing data",
            "path": "Samples~/Basic"
        },
        {
            "displayName": "ChangeScene",
            "description": "Demo for normal or additive scene switching",
            "path": "Samples~/ListServer"
        },
        {
            "displayName": "InterestManagement",
            "description": "Demo for interest management",
            "path": "Samples~/InterestManagement"
        },
        {
            "displayName": "Chat",
            "description": "Simple chat and custom UI demo",
            "path": "Samples~/Chat"
        },
        {
            "displayName": "Pong",
            "description": "Classic multiplayer game",
            "path": "Samples~/Pong"
        },
        {
            "displayName": "Tanks",
            "description": "Shoot each other with tanks",
            "path": "Samples~/Tanks"
        }
    ]
}
